@model IEnumerable<OdeToFood.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <!--<div>There are @Enumerable.Count(Model) Restaurants in database</div>-->
    <div>There are @Model.Count() Restaurants in database</div>
    <ul>
        @foreach(var restaurant in Model)
         {
                <li>@restaurant.Name</li>
         }
    </ul>
</div>